############## FULL SUPPORT #####################

#[chrome.local]
#mode=[Local or Remote]
#driver=[Chrome, Firefox, Edge, Safari, or PhantomJS]
#executable=[path to driver executable file]
#remoteUrl=[url of selenium HUB] 
#capabilities=[define capabilities for each browser]
#arguments=[The arguments to use when starting browser]

##### ONLY FOR CHROME DRIVER #####
#experimentalOptions=[define experimentalOptions for Chrome browser]


##################################===LOCAL===###############################
[chrome.local]
mode=Local
platform=Desktop
driver=Chrome
provider=Selenium

[firefox.local]
mode=Local
platform=Desktop
driver=Firefox
provider=Selenium

[ie.local]
mode=Local
platform=Desktop
driver=IE
provider=Selenium

[edge.local]
mode=Local
platform=Desktop
driver=Edge
provider=Selenium
executable=src/test/resources/drivers/win/msedgedriver.exe
arguments={"arg1":"--disable-features=RendererCodeIntegrity"}

##################################===REMOTE===###############################
[chrome.remote]
mode=Remote
platform=Desktop
driver=Chrome
provider=Selenium
remoteUrl=http://192.168.171.71:4444/wd/hub
capabilities={"browserName":"chrome","platform":"ANY"}

[firefox.remote]
mode=Remote
platform=Desktop
driver=Firefox
provider=Selenium
remoteUrl=http://192.168.171.71:4444/wd/hub
capabilities={"browserName":"firefox","platform":"ANY"}

[ie.remote]
