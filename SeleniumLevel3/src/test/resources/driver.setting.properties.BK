##############################===MOBILE===########################################
[android.chrome.local]
mode=Local
platform=Desktop
driver=Chrome
provider=Selenium
######## Chrome browser on Android without using Appium ########
experimentalOptions={"androidPackage": "com.android.chrome"}
######## Samsung Internet browser on Android without using Appium ########
#experimentalOptions={"androidPackage": "com.sec.android.app.sbrowser", "androidActivity": "com.sec.android.app.sbrowser.SBrowserMainActivity", "androidDeviceSocket": "Terrace_devtools_remote", "androidExecName": "Terrace"}

[android.chrome]
mode=Remote
platform=Mobile
driver=Chrome
provider=Selenium
remoteUrl=http://127.0.0.1:4723/wd/hub
capabilities={"browserName": "chrome", "deviceName": "Samsung Tab", "platformName":"Android", "udid": "c57bf216"}

[android.samsunginternet]
mode=Remote
platform=Mobile
driver=Chrome
provider=Selenium
remoteUrl=http://127.0.0.1:4723/wd/hub
capabilities={"browserName": "chrome", "chromedriverExecutable": "${user.dir}/src/test/resources/drivers/android/chromedriver.exe", "deviceName": "Samsung Tab", "platformName":"Android", "udid": "c57bf216"}
experimentalOptions={"androidPackage": "com.sec.android.app.sbrowser", "androidActivity": "com.sec.android.app.sbrowser.SBrowserMainActivity", "androidDeviceSocket": "Terrace_devtools_remote", "androidExecName": "Terrace"}

[android.firefox]
mode=Remote
platform=Mobile
driver=Firefox
provider=Selenium
remoteUrl=http://127.0.0.1:4723/wd/hub
capabilities={"browserName": "firefox", "deviceName": "Samsung Tab", "platformName":"Android", "udid": "c57bf216"}

[ios.safari]
mode=Remote
platform=Mobile
driver=Safari
provider=Selenium
remoteUrl=http://192.168.171.71:4444/wd/hub
capabilities={"browserName": "safari", "platformVersion": "13.2.3", "deviceName": "Iphone 6s", "platformName": "ios", "udid": "468191f9f9e3a1743ddca8ed16375d02bc0858ab"}

##############################===HEADLESS===########################################
[chrome.local.headless]
mode=Local
platform=Desktop
driver=Chrome
provider=Selenium
arguments={"arg1":"--headless","arg2":"--disable-gpu","arg3":"--window-size=1280,1024"}

[chrome.remote.headless]
mode=Remote
platform=Desktop
driver=Chrome
provider=Selenium
remoteUrl=http://localhost:4443/wd/hub
capabilities={"browserName":"chrome","platform":"ANY"}
arguments={"arg1":"--headless","arg2":"--disable-gpu","arg3":"--window-size=1280,1024"}

[firefox.local.headless]
mode=Local
platform=Desktop
driver=Firefox
provider=Selenium
executable=src/test/resources/drivers/win/geckodriver.exe
arguments={"arg1":"-headless"}
